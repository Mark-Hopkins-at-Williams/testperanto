{
    "distributions": [
        {"name": "vb", "type": "pyor", "strength": 5, "discount": 0},
        {"name": "vb.1", "type": "pyor", "strength": 5, "discount": 0},
        {"name": "vb.2", "type": "pyor", "strength": 5, "discount": 0},
        {"name": "vb.3", "type": "pyor", "strength": 5, "discount": 0},
        {"name": "vb.1.$y1", "type": "pyor", "strength": 5, "discount": 0},
        {"name": "vb.2.$y1", "type": "pyor", "strength": 5, "discount": 0},
        {"name": "vb.3.$y1", "type": "pyor", "strength": 5, "discount": 0},
        {"name": "nn", "type": "pyor", "strength": 5, "discount": 0},
        {"name": "nn.loc", "type": "pyor", "strength": 5, "discount": 0},
        {"name": "nn.time", "type": "pyor", "strength": 5, "discount": 0},
        {"name": "nn.arg0", "type": "pyor", "strength": 5, "discount": 0},
        {"name": "nn.arg1", "type": "pyor", "strength": 5, "discount": 0},
        {"name": "nn.arg2", "type": "pyor", "strength": 5, "discount": 0},
        {"name": "nn.arg0.$y1", "type": "pyor", "strength": 2, "discount": 0},
        {"name": "nn.arg1.$y1", "type": "pyor", "strength": 2, "discount": 0},
        {"name": "nn.arg2.$y1", "type": "pyor", "strength": 2, "discount": 0}
    ],
    "rules": [
        {"rule": "$qstart -> $qevent.0"},
        {"rule": "$qentity.arg0.$y1 -> (ENTITY $qnn.$z1 $qentitymods)", "base_weight": 0.2, "zdists": ["nn.arg0.$y1"]},
        {"rule": "$qentity.arg1.$y1 -> (ENTITY $qnn.$z1 $qentitymods)", "base_weight": 0.2, "zdists": ["nn.arg1.$y1"]},
        {"rule": "$qentity.arg2.$y1 -> (ENTITY $qnn.$z1 $qentitymods)", "base_weight": 0.2, "zdists": ["nn.arg2.$y1"]},
        {"rule": "$qentity.loc -> (ENTITY $qnn.$z1 $qentitymods)", "base_weight": 0.1, "zdists": ["nn.loc"]},
        {"rule": "$qentity.time -> (ENTITY $qnn.$z1 $qentitymods)", "base_weight": 0.1, "zdists": ["nn.time"]},
        {"rule": "$qevent.$y1 -> (EVENT $qvb.$z1 $qargs.$z1 $qmods)",  "base_weight": 0.6, "zdists": ["vb.2.$y1"]},
        {"rule": "$qrcevent -> (EVENT $qvb.$z1 $qrcargs.$z1 $qmods)",  "base_weight": 0.6, "zdists": ["vb.2"]},
        {"rule": "$qccomp.$y1 -> (EVENT $qvb.$z1 $qargsccomp.$z1 $qmods)",  "base_weight": 0.6, "zdists": ["vb.2.$y1"]},
        {"rule": "$qvb.$y1 -> (inst vb.$y1)"},
        {"rule": "$qnn.$y1 -> (inst nn.$y1)"},
        {"rule": "$qargs.$y1 -> (args $qarg0.$y1 $qarg1.$y1)"},
        {"rule": "$qargsccomp.$y1 -> (args $qarg1ccomp.$y1)"},
        {"rule": "$qrcargs.$y1 -> (args $qarg1.$y1)"},
        {"rule": "$qarg0.$y1 -> (arg0 $qentity.arg0.$y1)", "base_weight": 0.9},
        {"rule": "$qarg1.$y1 -> (arg1 $qentity.arg1.$y1)", "base_weight": 0.8},
        {"rule": "$qarg1.$y1 -> (arg1 $qccomp.$y1)", "base_weight": 0.2},
        {"rule": "$qarg1ccomp.$y1 -> (arg1 $qentity.arg0.$y1)"},
        {"rule": "$qarg2.$y1 -> (arg2 $qentity.arg2.$y1)"},
        {"rule": "$qentitymods -> (mods -null-)"},
        {"rule": "$qentitymods -> (mods $qentitymod)", "discount_factor": 0.5},
        {"rule": "$qentitymod -> (arg0-of $qrcevent)"},
        {"rule": "$qmods -> (mods $qlocation)"},
        {"rule": "$qmods -> (mods $qtime)"},
        {"rule": "$qmods -> (mods $qpolarity)"},
        {"rule": "$qpolarity -> (polarity -)"},
        {"rule": "$qlocation -> (location $qentity.loc)"},
        {"rule": "$qtime -> (time $qentity.time)"}
    ]
}
